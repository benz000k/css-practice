 {{#with data}}
    <div class="card">
                <div class="card-header">
                    {{name}}
                </div>
                <div class="card-body">
                    <div class="card_block">
                    <span class="card_price_amount">
                        ${{price}}
                    </span>
                    <span class="card_price_duration">/ mo</span>
                    </div>
                    <div class="card_block">
                    {{#each features}}
                        <div class="card_description_item">{{this}}</div>
                    {{/each}}
                    </div>
                    <div>
                    <button class="btn {{#if @first}}btn-outline-primary{{else}}btn-primary{{/if}} card_button">{{button}}</button>
                    </div>
                </div>
    </div>
 {{/with}}